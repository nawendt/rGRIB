#!/bin/sh

PKG_CPPFLAGS=
PKG_LIBS=

if test "X$PKG_CPPFLAGS" = "X" || test "X$PKG_LIBS" = "X"; then
  echo "PKG_CPPFLAGS=" > src/Makevars.win
  echo "PKG_LIBS=-leccodes" >> src/Makevars.win

else
  echo "PKG_CPPFLAGS=$PKG_CPPFLAGS" > src/Makevars.win
  echo "PKG_LIBS=$PKG_LIBS" >> src/Makevars.win
fi
